[launcher]
        [set_launcher]
                # Limit size of the launcher window (px)
                width_limit = 300
 
                name = "launcher_exec"
                prompt = "Exec: "
                command = "exec"
        [/set_launcher]
 
        #ViWMFS : manage wmfs with vi-based command.
        [set_launcher]
                name = "viwmfs"
                prompt = "&gt; "
                command = "wmfs -V"
        [/set_launcher]
[/launcher]

